# ABSA Service 환경 변수 설정

# 서버 설정
PORT=8003
DEBUG=true

# 데이터베이스 설정
DATABASE_URL=postgresql://postgres:password@localhost:5432/pension_sentiment

# Redis 캐시 설정
REDIS_URL=redis://localhost:6379

# 외부 서비스 연동
ANALYSIS_SERVICE_URL=http://localhost:8001

# 요청 처리 설정
MAX_CONCURRENT_REQUESTS=5
REQUEST_TIMEOUT=30

# ABSA 모델 설정
MODEL_CACHE_SIZE=100
ASPECT_EXTRACTION_CONFIDENCE=0.7
SENTIMENT_CONFIDENCE=0.6