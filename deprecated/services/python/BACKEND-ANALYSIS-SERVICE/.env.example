# Analysis Service 환경 변수 설정

# 서버 설정
PORT=8001
DEBUG=true
ENVIRONMENT=development

# 데이터베이스 설정
DATABASE_URL=postgresql://postgres:password@localhost:5432/analysis_db

# Redis 캐시 설정
REDIS_URL=redis://localhost:6379

# 모델 및 분석 설정
MAX_CONCURRENT_JOBS=5
JOB_TIMEOUT=600

# 서비스 디스커버리 (Eureka)
EUREKA_ENABLED=false
EUREKA_SERVICE_URLS=http://localhost:8761/eureka
EUREKA_APP_NAME=analysis-service
EUREKA_INSTANCE_HOST=
EUREKA_INSTANCE_IP=
EUREKA_METADATA={"version":"v1"}

# Consul 설정 (환경 설정 로드)
CONSUL_HTTP_ADDR=http://localhost:8500
CONSUL_HTTP_TOKEN=
CONSUL_HTTP_SSL_VERIFY=true
CONSUL_HTTP_TIMEOUT=5
